{
  "name": "babel-typescript-sample",
  "version": "0.7.0",
  "license": "MIT",
  "scripts": {
    "type-check": "tsc --noEmit",
    "ts:type-check:watch": "npm run type-check -- --watch",
    "ts:build": "npm run build:types && npm run build:js",
    "ts:build:types": "tsc --emitDeclarationOnly",
    "ts:build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "./node_modules/.bin/webpack --config prod.webpack.config.js --mode production",
    "builddev": "webpack-dev-server --config webpack.config.js --mode development --watch --open",
    "start": "npm run builddev"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/react-redux": "^7.0.8",
    "@types/redux-thunk": "^2.1.32",
    "autoprefixer": "^9.5.1",
    "awesome-typescript-loader": "^5.2.1",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "extract-loader": "^3.1.0",
    "file-loader": "^3.0.1",
    "html-webpack-change-assets-extension-plugin": "^1.2.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "svg-spritemap-webpack-plugin": "^3.3.0",
    "terser-webpack-plugin": "^1.2.3",
    "typescript": "^3.4.5",
    "webpack": "^4.31.0",
    "webpack-clean-plugin": "^0.2.3",
    "webpack-cli": "^3.3.2",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-hot-middleware": "^2.24.4"
  },
  "dependencies": {
    "@types/jest": "^24.0.12",
    "@types/node": "^12.0.0",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "axios": "^0.18.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  }
}
